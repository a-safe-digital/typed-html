{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "lib": ["ESNext"],
    "moduleResolution": "NodeNext",
    "declaration": true,
    "composite": true,
    "declarationMap": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitThis": true,
    "types": [],
    "paths": {
      "typed-html": ["../src/index.ts"],
      "typed-html/types": ["../src/types.ts"],
      "typed-html/jsx-runtime": ["../src/jsx-runtime.ts"],
      "typed-html/elements": ["../src/elements.ts"]
    }
  }
}
